{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "_assets": "./src/Assets",
          "_components": "./src/Components",
          "_constants": "./src/Constants",
          "_navigations": "./src/Navigations",
          "_screens": "./src/Screens",
          "_services": "./src/Services",
          "_styles": "./src/Styles",
          "_utils": "./src/Utils"
        }
      }
    ]
  ]
}